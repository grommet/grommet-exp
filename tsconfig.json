{
  "compilerOptions": {
    "target": "es5",
    "module": "ESNext",
    "jsx": "react-jsx",
    "sourceMap": true,
    "outDir": "dist",
    "lib": [ "es6" ],
    "rootDir": "src",
    "declaration": true,
    "declarationDir": "./dist",
    "strict": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "noEmitHelpers": true,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "moduleResolution": "node"
  },
  "exclude": [
    "node_modules",
    "dist"
  ],
}